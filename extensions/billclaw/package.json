  "author": "fire-zu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fire-zu/billclaw.git",
    "directory": "extensions/billclaw"
  },
  "peerDependencies": {
    "openclaw": "*"
  },
  "devDependencies": {
    "@openclaw/plugin-sdk": "*",
    "openclaw": "*",
    "typescript": "^5.8.0",
    "vitest": "^3.0.0",
    "oxlint": "^0.15.0",
    "oxfmt": "^0.1.0"
  },
  "dependencies": {
    "plaid": "^32.0.0",
    "zod": "^3.24.0"
