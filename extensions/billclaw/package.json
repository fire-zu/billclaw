{
  "name": "@fire-zu/billclaw",
  "version": "0.0.1",
  "description": "Bank transaction and bill data import for OpenClaw - Data sovereignty for your financial data",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "openclaw.plugin.json"
  ],
  "scripts": {
